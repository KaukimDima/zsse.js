[
    {
        "description": "Default buttons\n",
        "commentRange": {
            "start": 348,
            "end": 348
        },
        "context": {
            "type": "css",
            "name": ".#{$inuit-btn-namespace}btn,\n%#{$inuit-btn-namespace}btn",
            "value": "display: inline-block;\n  overflow: visible;\n  height: $inuit-btn-height;\n  min-width: $inuit-btn-min-width;\n  margin: 0;\n  border: $inuit-btn-border $inuit-btn-border-color;\n  border-radius: 0 !important;\n  padding: 0 calc(#{$inuit-btn-height} / 2);\n  box-shadow: $inuit-btn-shadow--light;\n  font: inherit;\n  line-height: calc(#{$inuit-btn-height} - 2px);\n  -webkit-font-smoothing: antialiased;\n  cursor: pointer;\n  text-align: center;\n  text-decoration: none;\n  text-transform: none;\n  white-space: nowrap;\n  background-color: $inuit-btn-background;\n  transition: background .4s, border-color .4s, color .4s;\n  &,\n  &:link,\n  &:visited,\n  &:hover,\n  &:active {\n    color: $inuit-btn-color;\n  }\n  &:hover,\n  &:focus {\n    border-color: $inuit-btn-border-color--hover;\n    box-shadow: $inuit-btn-shadow;\n    background-color: $inuit-btn-background--hover;\n  }\n  &:active {\n    border-color: $inuit-btn-border-color--pressed;\n    box-shadow: none;\n    background-color: $inuit-btn-background--pressed;\n  }\n  &:active {\n    outline: none;\n  }",
            "line": {
                "start": 350,
                "end": 665
            }
        },
        "group": [
            "px-buttons-design"
        ],
        "access": "public",
        "file": {
            "path": "_objects.buttons.scss",
            "name": "_objects.buttons.scss"
        }
    },
